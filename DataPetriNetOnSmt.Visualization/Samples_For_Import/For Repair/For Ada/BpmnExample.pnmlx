      </page>
      <variables>
         <variable maxValue="100000" minValue="0" type="java.lang.Double">
            <name>amount</name>
         </variable>
         <variable maxValue="100000" minValue="0" type="java.lang.Double">
            <name>age</name>
         </variable>
         <variable maxValue="100000" minValue="0" type="java.lang.Double">
            <name>salary</name>
         </variable>
         <variable maxValue="100000" minValue="0" type="java.lang.Double">
            <name>repayment</name>
         </variable>
         <variable type="java.lang.Boolean">
            <name>goodhistory</name>
         </variable>
      </variables>
   </net>
