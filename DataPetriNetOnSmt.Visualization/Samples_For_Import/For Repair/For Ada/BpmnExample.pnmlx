<?xml version="1.0" encoding="UTF-8"?>
<pnml>
   <net id="net1" type="http://www.pnml.org/version-2009/grammar/pnmlcoremodel">
      <name>
         <text>Credit Bank Request</text>
      </name>
      <page id="n0">
         <place id="i">
            <name>
               <text>i</text>
            </name>
            <initialMarking>
               <text>1</text>
            </initialMarking>
         </place>
         <place id="p1">
            <name>
               <text>p1</text>
            </name>
         </place>
         <place id="p2">
            <name>
               <text>p2</text>
            </name>
         </place>
         <place id="p3">
            <name>
               <text>p3</text>
            </name>
         </place>    
		<place id="p4">
            <name>
               <text>p4</text>
            </name>
         </place> 
		<place id="p5">
            <name>
               <text>p5</text>
            </name>
         </place> 
		<place id="p6">
            <name>
               <text>p6</text>
            </name>
         </place> 
		<place id="p7">
            <name>
               <text>p7</text>
            </name>
         </place> 	
         <place id="p8">
            <name>
               <text>p8</text>
            </name>
         </place> 	 
         <place id="o">
            <name>
               <text>o</text>
            </name>
			<finalMarking>
               <text>1</text>
            </finalMarking>
         </place>		 
         <transition id="t0" guard="((amount' &gt; 0) &amp;&amp; (age' &gt; 0) &amp;&amp; (salary' &gt; 0))">
            <name>
               <text>Loan Request</text>
            </name>
         </transition>
		 <transition id="t1">
            <name>
               <text>AndS</text>
            </name>
         </transition>
         <transition guard="((repayment' &gt; 0))" id="t2">
            <name>
               <text>Repayment Computation</text>
            </name>
         </transition>
		 <transition guard="((goodhistory' == True) || (goodhistory' == False))" id="t3">
            <name>
               <text>History Evaluation</text>
            </name>
         </transition>
		 <transition id="t4">
            <name>
               <text>AndJ</text>
            </name>
         </transition>
         <transition id="t5">
            <name>
               <text>Early Rejection</text>
            </name>
         </transition>
		 <transition id="t6" guard="((goodhistory == True) &amp;&amp; (age &gt;= 18))">
            <name>
               <text>Preliminary Approval</text>
            </name>
         </transition>
         <transition id="t7">
            <name>
               <text>Detailed Investigation</text>
            </name>
         </transition>
		 <transition id="t8" guard="(repayment &gt;= salary)">
            <name>
               <text>Rejection</text>
            </name>
         </transition>

         <transition id="t9" guard="((repayment &lt; salary) &amp;&amp; (salary &gt; 1000))">
            <name>
               <text>Loan Issue</text>
            </name>
         </transition>
		       
         <arc id="arc0" source="i" target="t0">
            <name>
               <text>1</text>
            </name>
         </arc>
