<?xml version="1.0" encoding="UTF-8"?>
<pnml>
   <net id="net1" type="http://www.pnml.org/version-2009/grammar/pnmlcoremodel">
      <name>
         <text>Boundness check</text>
      </name>
      <page id="n0">
         <place id="i">
            <name>
               <text>i</text>
            </name>
            <initialMarking>
               <text>1</text>
            </initialMarking>
         </place>
		 
		 <place id="p0">
            <name>
               <text>p0</text>
            </name>
         </place>
		 <place id="p1">
            <name>
               <text>p1</text>
            </name>
         </place>
         
         <place id="o">
            <name>
               <text>o</text>
            </name>
			<finalMarking>
               <text>1</text>
            </finalMarking>
         </place>
         
         <transition id="t0" guard="c_w &gt; 0 &amp;&amp; c_w &lt; 10">
            <name>
               <text>t0</text>
            </name>
         </transition>  
		<transition id="t1_1" guard="b_w &lt; c_r &amp;&amp; b_w &gt; 3 || b_w &lt; c_r &amp;&amp; a_r == 0">
            <name>
               <text>t1_1</text>
            </name>
         </transition> 	
		 <transition id="t1_2" guard="b_w &lt; c_r &amp;&amp; b_w &lt;= 3 &amp;&amp; a_r != 0">
            <name>
               <text>t1_2</text>
            </name>
         </transition> 
		<transition id="t2" guard="b_r &gt; 3 || a_r == 0">
            <name>
               <text>t2</text>
            </name>
         </transition>
		<transition id="t3_1" guard="a_w != 0 &amp;&amp; b_r &gt; 3">
            <name>
               <text>t3_1</text>
            </name>
         </transition> 	
		 <transition id="t3_2" guard="a_w != 0 &amp;&amp; b_r &lt;= 3">
            <name>
               <text>t3_2</text>
            </name>
         </transition>		 
		 
         
         <arc id="arc0" source="i" target="t0">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc1" source="t0" target="p0">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc2" source="p0" target="t1_1">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc3" source="t1_1" target="p1">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc2" source="p0" target="t1_2">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc3" source="t1_2" target="p1">
            <name>
               <text>1</text>
            </name>
         </arc>
		 
		 <arc id="arc2" source="p1" target="t3_1">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc3" source="t3_1" target="p0">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc2" source="p1" target="t3_2">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc3" source="t3_2" target="p0">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc4" source="p0" target="t2">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc3" source="t2" target="o">
            <name>
               <text>1</text>
            </name>
         </arc>   
      </page>
      <variables>
         <variable type="Real">
            <name>a</name>
         </variable>
         <variable type="Real">
            <name>b</name>
         </variable>
		 <variable type="Real">
            <name>c</name>
         </variable>
      </variables>
   </net>
</pnml>