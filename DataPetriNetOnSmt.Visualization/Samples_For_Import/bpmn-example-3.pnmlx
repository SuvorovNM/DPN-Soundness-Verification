		 <arc id="arc16" source="p7" target="t7">
            <name>
               <text>1</text>
            </name>
         </arc>
         <arc id="arc16" source="p7" target="t8">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc14" source="t7" target="o">
            <name>
               <text>1</text>
            </name>
         </arc>	 

         <arc id="arc14" source="t8" target="o">
            <name>
               <text>1</text>
            </name>
         </arc>
      </page>
      <variables>
         <variable type="Real">
            <name>amount</name>
         </variable>
		 <variable type="Real">
            <name>age</name>
         </variable>
		 <variable type="Real">
            <name>salary</name>
         </variable>
		 <variable type="Real">
            <name>repayment</name>
         </variable>
         <variable type="Boolean">
            <name>goodhistory</name>
         </variable>
      </variables>
   </net>
</pnml>