<?xml version="1.0" encoding="UTF-8"?>
<pnml>
   <net id="net1" type="http://www.pnml.org/version-2009/grammar/pnmlcoremodel">
      <name>
         <text>Credit Bank Request</text>
      </name>
      <page id="n0">
         <place id="i">
            <name>
               <text>i</text>
            </name>
            <initialMarking>
               <text>1</text>
            </initialMarking>
         </place>
         <place id="p1">
            <name>
               <text>p1</text>
            </name>
         </place>
         <place id="p2">
            <name>
               <text>p2</text>
            </name>
         </place>
         <place id="p3">
            <name>
               <text>p3</text>
            </name>
         </place>    
		<place id="p4">
            <name>
               <text>p4</text>
            </name>
         </place> 
		<place id="p5">
            <name>
               <text>p5</text>
            </name>
         </place> 
		<place id="p6">
            <name>
               <text>p6</text>
            </name>
         </place> 
		<place id="p7">
            <name>
               <text>p7</text>
            </name>
         </place> 		 
         <place id="o">
            <name>
               <text>o</text>
            </name>
			<finalMarking>
               <text>1</text>
            </finalMarking>
         </place>		 
         <transition id="t0" guard="amount_w &gt; 0 &amp;&amp; age_w &gt; 0 &amp;&amp; salary_w &gt; 0">
            <name>
               <text>Loan Request</text>
            </name>
         </transition>
		 <transition id="t1">
            <name>
               <text>AndS</text>
            </name>
         </transition>
         <transition guard="repayment_w &gt; 0" id="t2">
            <name>
               <text>Repayment Computation</text>
            </name>
         </transition>
		 <transition guard="goodhistory_w == True || goodhistory_w == False" id="t3">
            <name>
               <text>History Evaluation</text>
            </name>
         </transition>
		 <transition id="t4">
            <name>
               <text>AndJ</text>
            </name>
         </transition>
         <transition id="t5">
            <name>
               <text>Early Rejection</text>
            </name>
         </transition>
		 <transition id="t6" guard="goodhistory_r == True &amp;&amp; age_r &gt;= 18">
            <name>
               <text>Detailed Investigation</text>
            </name>
         </transition>
		 <transition id="t7" guard="repayment_r &gt;= salary_r">
            <name>
               <text>Rejection</text>
            </name>
         </transition>

         <transition id="t8" guard="repayment_r &lt; salary_r &amp;&amp; salary_r > 1000">
            <name>
               <text>Loan Issue</text>
            </name>
         </transition>
		       
         <arc id="arc0" source="i" target="t0">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc1" source="t0" target="p1">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc2" source="p1" target="t1">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc3" source="t1" target="p2">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc4" source="t1" target="p3">
            <name>
               <text>1</text>
            </name>
         </arc>

		 <arc id="arc5" source="p2" target="t2">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc6" source="p3" target="t3">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc7" source="t3" target="p5">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc8" source="t2" target="p4">
            <name>
               <text>1</text>
            </name>
         </arc>
		 
		 
		 
		 <arc id="arc9" source="p4" target="t4">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc10" source="p5" target="t4">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc11" source="t4" target="p6">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc12" source="p6" target="t5">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc13" source="p6" target="t6">
            <name>
               <text>1</text>
            </name>
         </arc>
		 <arc id="arc14" source="t5" target="o">
            <name>
               <text>1</text>
